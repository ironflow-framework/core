<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>IronFlow Framework Documentation</title>
    <parser>
        <target>docs/cache</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
            <item>@todo</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <ignore>
            <item>vendor/*</item>
            <item>tests/*</item>
        </ignore>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <transformations>
        <template name="clean" />
    </transformations>
</phpdoc> 